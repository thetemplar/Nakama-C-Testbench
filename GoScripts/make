export GOROOT=/usr/local/go
export GOPATH=$HOME/go                                                                                                                                                                                                         
export PATH=$GOPATH/bin:$GOROOT/bin:$PATH 

rm some_project.so &> /dev/null
cd ../some_project
protoc --go_out=. --csharp_out=. matchstate.proto 
chmod 777 *.go
go build --buildmode=plugin -o ~/go/src/modules/some_project.so &&  ~/go/src/github.com/heroiclabs/nakama/nakama --runtime.path ~/go/src/modules  
#--logger.level "debug"           
